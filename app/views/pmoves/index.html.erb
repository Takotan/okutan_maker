<p id="notice"><%= notice %></p>

<h1>Pmoves</h1>

<table>
  <thead>
    <tr>
      <th>Pokemon</th>
      <th>Move</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmoves.each do |pmove| %>
      <tr>
        <td><%= pmove.pokemon_id %></td>
        <td><%= pmove.move_id %></td>
        <td><%= link_to 'Show', pmove %></td>
        <td><%= link_to 'Edit', edit_pmove_path(pmove) %></td>
        <td><%= link_to 'Destroy', pmove, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pmove', new_pmove_path %>
